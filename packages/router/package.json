{
  "name": "@ingress/router",
  "version": "0.1.0-alpha.2",
  "keywords": [
    "radix",
    "trie",
    "tree",
    "router",
    "path",
    "http route"
  ],
  "author": "Caleb Boyd <caleb.boyd@hotmail.com> (https://github.com/calebboyd)",
  "license": "MIT",
  "description": "A fast router middleware for ingress",
  "type": "commonjs",
  "types": "./lib/cjs/router.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/router.js",
        "require": "./lib/cjs/router.js"
      },
      "default": "./lib/esm/router.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "docs": "typedoc src/router.ts --excludePrivate",
    "build:esm": "tsc --sourceRoot \"$(pwd)/src\" --outDir lib/esm --module es2020 && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build": "shx rm -rf lib && tsc --sourceRoot \"$(pwd)/src\" && npm run build:esm"
  },
  "dependencies": {
    "@ingress/core": "1.0.0-rc.1",
    "@ingress/types": "0.0.1",
    "reflect-annotations": "4.0.0-alpha.6",
    "router-tree-map": "^0.1.2",
    "secure-json-parse": "^2.4.0"
  },
  "devDependencies": {
    "@hapi/shot": "^5.0.5",
    "@ingress/http": "0.0.1-alpha.0",
    "@types/hapi__shot": "^4.1.2"
  }
}
