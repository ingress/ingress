{
  "name": "@ingress/types",
  "version": "0.0.1",
  "license": "MIT",
  "type": "commonjs",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/index.js",
        "require": "./lib/cjs/index.js"
      },
      "default": "./lib/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "npm": ">=7",
    "node": ">=14"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "uvu -r ts-node/register src .*\\.test\\.ts",
    "tcov": "c8 --src src -r html npm test && open coverage/index.html",
    "build:esm": "tsc --sourceRoot \"$(pwd)/src\" --outDir lib/esm --module es2020 && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build": "shx rm -rf lib && tsc --sourceRoot \"$(pwd)/src\" && npm run build:esm"
  },
  "dependencies": {}
}
