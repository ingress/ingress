{
  "name": "ingress",
  "version": "1.0.0-beta.16",
  "description": "Composable javascript applications",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./lib/esm/ingress.js",
      "require": "./lib/cjs/ingress.js",
      "types": {
        "import": "./lib/esm/ingress.d.ts",
        "require": "./lib/cjs/ingress.d.ts"
      }
    },
    "./typebox": {
      "import": "./lib/esm/typebox.js",
      "require": "./lib/cjs/typebox.js",
      "types": {
        "import": "./lib/esm/typebox.d.ts",
        "require": "./lib/cjs/typebox.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.mjs",
    "lib"
  ],
  "scripts": {
    "test": "vitest run src",
    "cov": "$npm_execpath test -- --coverage && open coverage/index.html",
    "tsc": "node ../../scripts/esm.mjs lib/esm/package.json",
    "build": "shx rm -rf lib && $npm_execpath run tsc"
  },
  "repository": {
    "directory": "packages/ingress",
    "type": "git",
    "url": "ssh://git@github.com/ingress/ingress.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "server",
    "http",
    "application",
    "framework",
    "composable",
    "declarative",
    "middleware"
  ],
  "dependencies": {
    "@ingress/core": "workspace:*",
    "@ingress/http": "workspace:*",
    "@ingress/router": "workspace:*",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.31.14"
  },
  "peerDependenciesMeta": {
    "@sinclair/typebox": {
      "optional": true
    }
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.31.14",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@hapi/shot": "6.0.1"
  }
}