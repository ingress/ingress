{
  "name": "ingress",
  "version": "5.0.0-alpha.10",
  "description": "Composable nodejs applications",
  "type": "commonjs",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/main.js",
        "require": "./lib/cjs/main.js"
      },
      "default": "./lib/cjs/main.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:esm": "tsc --sourceRoot \"$(pwd)/src\" --outDir lib/esm --module es2020 && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build": "shx rm -rf lib && tsc --sourceRoot \"$(pwd)/src\" && npm run build:esm",
    "watch:typescript": "tsc -w",
    "watch:test": "jest --watchAll --detectOpenHandles",
    "watch": "npm run build && concurrently -k \"npm:watch:typescript\" \"npm:watch:test\" -c blue,green",
    "test": "jest"
  },
  "repository": {
    "directory": "modules/ingress",
    "type": "git",
    "url": "ssh://git@github.com/ingress/ingress.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "jest": {
    "resolver": "jest-ts-webcompat-resolver",
    "setupFilesAfterEnv": [
      "reflect-metadata"
    ],
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      ".*\\.js",
      ".*\\.util\\.spec"
    ]
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "server",
    "http",
    "application",
    "framework",
    "composable",
    "declarative",
    "middleware"
  ],
  "dependencies": {
    "@hapi/subtext": "~7.0.3",
    "@ingress/di": "3.0.0-alpha.2",
    "@ingress/http-status": "3.0.0-alpha.6",
    "app-builder": "^7.0.4",
    "destroy": "~1.1.0",
    "find-my-way": "^5.1.1",
    "on-finished": "~2.3.0",
    "reflect-annotations": "^4.0.0-alpha.6",
    "rxjs": "~7.5.2",
    "secure-json-parse": "^2.4.0",
    "uuid": "^8.3.2",
    "ws": "^8.4.2"
  },
  "peerDependencies": {
    "@types/ws": "^8.2.2"
  },
  "peerDependenciesMeta": {
    "@types/ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/destroy": "~1.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/on-finished": "~2.3.1",
    "@types/secure-json-parse": "^1.0.3",
    "@types/sinon": "^10.0.8",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^7.0.0",
    "cross-fetch": "~3.1.5",
    "get-port": "^5",
    "jest": "^27.3.1",
    "jest-github-actions-reporter": "^1.0.3",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sinon": "^12.0.1",
    "ts-jest": "^27.0.7"
  },
  "gitHead": "4bc0a9eb661fce02765d56efdc84f8d1a10a01ec"
}
