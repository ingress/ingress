{
  "name": "router-tree-map",
  "version": "0.1.2",
  "keywords": [
    "radix",
    "trie",
    "tree",
    "router",
    "path",
    "http route"
  ],
  "author": "Caleb Boyd <caleb.boyd@hotmail.com> (https://github.com/calebboyd)",
  "license": "MIT",
  "description": "A radix tree map-like structure for /route/:like/keys and values",
  "type": "commonjs",
  "main": "./lib/cjs/tree.js",
  "types": "./lib/cjs/tree.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/tree.js",
        "require": "./lib/cjs/tree.js"
      },
      "default": "./lib/cjs/tree.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "docs": "typedoc src/tree.ts --excludePrivate",
    "test": "uvu -r ts-node/register src .*\\.test\\.ts",
    "cov": "c8 --100 --src src -r html npm test",
    "ocov": "npm run cov && open coverage/index.html",
    "build:esm": "tsc --sourceRoot \"$(pwd)/src\" --outDir lib/esm --module es2020 && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build": "shx rm -rf lib && npm run build:esm && tsc --sourceRoot \"$(pwd)/src\""
  },
  "eslintConfig": {
    "rules": {
      "no-constant-condition": 0,
      "@typescript-eslint/no-this-alias": 0
    }
  },
  "gitHead": "4bc0a9eb661fce02765d56efdc84f8d1a10a01ec"
}
