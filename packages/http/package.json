{
  "name": "@ingress/http",
  "version": "0.0.1-alpha.0",
  "keywords": [],
  "author": "Caleb Boyd <caleb.boyd@hotmail.com> (https://github.com/calebboyd)",
  "license": "MIT",
  "description": "Core ingress objects, application lifecycle and helpers",
  "type": "commonjs",
  "types": "./lib/cjs/node.http.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/node.http.js",
        "require": "./lib/cjs/node.http.js"
      }
    }
  },
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "test": "uvu -r ts-node/register -r reflect-metadata src .*\\.test\\.ts -- --no-warnings",
    "cov": "c8 --src src -r html npm test && open coverage/index.html",
    "build:esm": "tsc --sourceRoot \"$(pwd)/src\" --outDir lib/esm --module es2020 && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build": "shx rm -rf lib && npm run build:esm && tsc --sourceRoot \"$(pwd)/src\""
  },
  "dependencies": {
    "@ingress/core": "1.0.0-rc.1",
    "@ingress/types": "0.0.1",
    "readable-stream": "^3.6.0",
    "secure-json-parse": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "undici": "^4.12.2"
  }
}
