{
  "name": "@ingress/core",
  "version": "1.0.0-rc.1",
  "keywords": [],
  "author": "Caleb Boyd <caleb.boyd@hotmail.com> (https://github.com/calebboyd)",
  "license": "MIT",
  "description": "Core ingress objects, application lifecycle and http handlers",
  "type": "commonjs",
  "types": "lib/cjs/core.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/esm/core.js",
        "require": "./lib/cjs/core.js"
      }
    }
  },
  "files": [
    "lib",
    "!lib/*.test.ts",
    "docs"
  ],
  "scripts": {
    "test": "uvu -r ts-node/register src .*\\.test\\.ts",
    "cov": "c8 --100 --src src -r html npm test",
    "ocov": "npm run cov && open coverage/index.html",
    "build:esm": "tsc --sourceRoot \"$(pwd)/src\" --outDir lib/esm --module es2020 && echo '{\"type\": \"module\"}' > lib/esm/package.json",
    "build": "shx rm -rf lib && npm run build:esm && tsc --sourceRoot \"$(pwd)/src\""
  },
  "dependencies": {
    "injection-js": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12"
  }
}
